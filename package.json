{
  "name": "shiaiboard",
  "version": "1.3.0",
  "license": "MIT",
  "description": "tomiki aikido shiai scoreboard",
  "author": "Carlos Aguilar <hey@carlos-aguilar.com>",
  "main": "index.js",
  "private": true,
  "keywords": [
    "aikido",
    "tomiki",
    "scoreboard",
    "shiai"
  ],
  "scripts": {
    "start": "bankai start index.js",
    "gen": "choo g",
    "pretty": "node pretty.js",
    "build": "bankai build index.js && cp CNAME dist/",
    "deploy": "gh-pages -d dist",
    "lint": "standard --verbose | snazzy",
    "deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons",
    "test": "npm run lint && npm run deps",
    "inspect": "bankai inspect index.js",
    "clean": "rm -r dist/"
  },
  "dependencies": {
    "choo": "^6.13.1",
    "choo-persist": "^3.0.2",
    "choo-service-worker": "^2.4.0",
    "sheetify": "^7.3.3",
    "tachyons": "^4.11.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "bankai": "^9.15.0",
    "choo-cli": "^2.1.0",
    "choo-devtools": "^2.5.1",
    "dependency-check": "^3.3.0",
    "gh-pages": "^2.0.1",
    "pretty": "^2.0.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
